{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/ui/components/forms/formLogin.jsx", "src/data/services/ApiService.jsx", "src/ui/components/Cadastro/index.jsx", "src/ui/components/Admin/index.jsx", "src/pages/[lc]/admin.jsx"],
  "exclude": ["node_modules"]
}
